{
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    "no-unused-vars": "warn",
    "@typescript-eslint/no-unused-vars": "warn",
    "react/jsx-no-target-blank": "warn",
    "no-undef": "warn",
    "no-console": "off"
  },
  "overrides": [
    {
      "files": ["**/*.test.js", "**/*.test.jsx", "**/*.test.ts", "**/*.test.tsx"],
      "rules": {
        "no-unused-vars": "off"
      }
    }
  ]
}
